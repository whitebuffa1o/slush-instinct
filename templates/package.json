  "version": "<%= appVersion %>",
  "private": true,
  "author": "<%= authorName %> <<%= authorEmail %>>",
  "repository": "<%= appRepo %>",
  "devDependencies": {
    "gulp": "^3.9.0",
    "instinct": "git@gitlab.com:clearlink/instinct.git",
    "imagemin-pngquant": "^4.1.2",
