    <% if(foundation) { %>
    "foundation-sites": "^5.5.2",
    <% } else { %>
    "node.normalize.scss": "^3.0.2",
    <% } %>
    "webpack": "^1.12.0",
    "yargs": "^3.21.0",
    "handlebars-layouts": "^3.1.2"
  }
}
